{
  "name": "integreat-queue-bull",
  "version": "0.1.4",
  "description": "Redis queue for Integreat - based on Bull",
  "keywords": [
    "redis",
    "bull",
    "queue",
    "integreat"
  ],
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no>",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && tsc && nyc --reporter=text-summary ava ./dist/**/*.test.js",
    "test:watch": "npm run dev",
    "dev": "ava --watch",
    "build": "tsc",
    "prepare": "npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nyc:report": "nyc report",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "sources": [
      "src/**/!(*.test).ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "include": [
      "**/!(*.test).js",
      "**/!(*.test|*.d).ts"
    ]
  },
  "engines": {
    "node": ">= 8.6",
    "npm": ">= 5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/integreat-io/integreat-queue-bull.git"
  },
  "bugs": {
    "url": "https://github.com/integreat-io/integreat-queue-bull/issues"
  },
  "homepage": "https://github.com/integreat-io/integreat-queue-bull#readme",
  "dependencies": {
    "@types/bull": "^3.10.0",
    "bull": "^3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@types/sinon": "^7.0.13",
    "ava": "2.2.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.5",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.3"
  }
}
